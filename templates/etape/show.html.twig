<!DOCTYPE html>

<title>Etape</title>

{% block body %}
    <h1>Etape</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ etape.id }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ etape.user }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ etape.email }}</td>
            </tr>
            <tr>
                <th>Sujet</th>
                <td>{{ etape.sujet }}</td>
            </tr>
            <tr>
                <th>Message</th>
                <td>{{ etape.message }}</td>
            </tr>
            <tr>
                <th>Delai</th>
                <td>{{ etape.delai }}</td>
            </tr>
            <tr>
                <th>Temps</th>
                <td>{{ etape.temps }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ etape.date ? etape.date|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Heure</th>
                <td>{{ etape.heure ? etape.heure|date('H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_etape_index') }}">back to list</a>

    <a href="{{ path('app_etape_edit', {'id': etape.id}) }}">edit</a>

    {{ include('etape/_delete_form.html.twig') }}
{% endblock %}
