<!DOCTYPE html>

<title>Tunnel</title>

{% block body %}
    <h1>Tunnel</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tunnel.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ tunnel.name }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ tunnel.active ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ tunnel.createdAt ? tunnel.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ tunnel.updatedAt ? tunnel.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_tunnel_index') }}">back to list</a>

    <a href="{{ path('app_tunnel_edit', {'id': tunnel.id}) }}">edit</a>

    {{ include('tunnel/_delete_form.html.twig') }}
{% endblock %}
